{
  "name": "tryTwitterTimeline",
  "version": "1.0.0",
  "main": "app.js",
  "author": "Morton358 <vovankovalchuk358@gmail.com>",
  "license": "MIT",
  "scripts": {
    "babel-node": "babel-node --presets env app.js",
    "client": "cd client && npm run start",
    "server": "nodemon --exec npm run babel-node -- app.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "build": "babel src -d dist",
    "heroku-postbuild-client": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "heroku-start": "npm install && cd client/ && npm install && cd .. && npm run dev"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^5.0.1",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "rimraf": "^2.6.2",
    "twit": "^2.2.9"
  }
}
